{
  "name": "@hackaton-rugby/frontend-app",
  "private": true,
  "version": "1.0.0",
  "license": "UNLICENSED",
  "scripts": {
    "analyze": "ANALYZE=true pnpm build",
    "bootstrap-frontend": "cdk bootstrap --profile dev-hackaton",
    "bootstrap-frontend-production": "cdk bootstrap --context stage=production",
    "bootstrap-frontend-staging": "cdk bootstrap --context stage=staging",
    "build": "vite build",
    "codegen": "graphql-codegen",
    "deploy-frontend": "cdk deploy --profile dev-hackaton",
    "deploy-frontend-production": "cdk deploy --context stage=production",
    "deploy-frontend-staging": "cdk deploy --context stage=staging",
    "format-check": "prettier --check . --ignore-path ../../.prettierignore",
    "format-fix": "prettier --write . --ignore-path ../../.prettierignore",
    "lint-fix": "pnpm linter-base-config --fix",
    "lint-fix-all": "pnpm lint-fix .",
    "linter-base-config": "eslint --ext=js,jsx,ts,tsx .",
    "serve": "vite preview",
    "start": "vite --port 3000",
    "stylelint-base-config": "stylelint",
    "stylelint-fix": "pnpm stylelint-base-config --fix",
    "test": "nx run format-check && nx run test-linter && nx run test-type && nx run test-unit && nx run test-circular",
    "test-circular": "pnpm depcruise --config -- .",
    "test-linter": "pnpm linter-base-config && pnpm stylelint-base-config **/*.{ts,tsx}",
    "test-type": "tsc",
    "test-unit": "vitest run --coverage --passWithNoTests"
  },
  "dependencies": {
    "@apollo/client": "^3.8.5",
    "@apollo/link-context": "2.0.0-beta.3",
    "@apollo/link-ws": "2.0.0-beta.3",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@mui/icons-material": "^5.10.16",
    "@mui/lab": "5.0.0-alpha.132",
    "@mui/material": "^5.10.17",
    "@mui/system": "^5.10.17",
    "@types/testing-library__jest-dom": "^5.14.5",
    "axios": "^1.2.2",
    "buffer": "^6.0.3",
    "graphql": "^16.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^6.2.5",
    "react-router-dom": "^6.4.5",
    "subscriptions-transport-ws": "^0.11.0",
    "tmi.js": "^1.8.5",
    "url": "^0.11.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^3.2.2",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@hackaton-rugby/cdk-configuration": "1.0.0",
    "@testing-library/dom": "^9.0.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/dotenv-flow": "^3.2.0",
    "@types/graphql": "^14.5.0",
    "@types/node": "^18.16.1",
    "@types/react": "^17.0.47",
    "@types/react-dom": "^18.0.0",
    "@types/tmi.js": "^1.8.4",
    "@types/uuid": "^9.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-c8": "0.30.0",
    "aws-cdk": "^2.96.2",
    "aws-cdk-lib": "^2.100.0",
    "constructs": "^10.3.0",
    "dependency-cruiser": "^13.0.0",
    "dotenv-flow": "^3.2.0",
    "eslint": "^8.29.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-risxss": "^2.1.0",
    "husky": "^8.0.2",
    "jsdom": "^22.0.0",
    "postcss": "^8.4.20",
    "postcss-styled-syntax": "^0.4.0",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.8.1",
    "rollup": "^3.20.7",
    "rollup-plugin-visualizer": "^5.9.0",
    "stylelint": "^15.0.0",
    "stylelint-config-property-sort-order-smacss": "^9.1.0",
    "stylelint-config-standard": "^30.0.0",
    "stylelint-order": "^6.0.3",
    "ts-node": "^10.8.1",
    "typescript": "^5.0.2",
    "vite": "^4.3.1",
    "vite-plugin-svgr": "^2.4.0",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "0.31.1"
  }
}
